{
    "manifest_version": 2,
    "name": "deflector",
    "version": "0.1",
    "description": "Like a deflector lightning strikes, this browser plugin is designed to divert disturbances in concentration.",
    "icons": {
        "48": "icons/umbrella.svg",
        "96": "icons/umbrella.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "deflect.js"
            ]
        }
    ]
}